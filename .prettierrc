{
	"overrides": [
		{
			"files": ["*.yml", "*.yaml"],
			"options": { "tabWidth": 2, "useTabs": false }
		}
	],
	"arrowParens": "always",
	"trailingComma": "es5",
	"endOfLine": "lf",
	"singleQuote": false,
	"bracketSpacing": true,
	"editorconfig": true,
	"useTabs": true,
	"tabWidth": 4,
	"semi": true
}
